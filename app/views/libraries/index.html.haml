.container
  %br
  .row
    = render partial: 'layouts/side_bar', locals: { active: 'libraries' }

    .span8
      %ul.grandham-items-list
        - @libraries.each do |library|
          = render partial: 'library', locals: { library: library }

      .list-pagination
        = will_paginate @libraries, renderer: BootstrapPagination::Rails